{
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "headers": [
    {
      "headers": [
        {
          "key": "access-control-allow-origin",
          "value": "https://nft-game-2-client.vercel.app"
        }
      ]
    }
  ]
}